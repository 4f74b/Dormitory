    <script>
        $('#modal-blockui').on('click', function () {
            var block = $('#modal-reload');
            $(block).block({
                message: '<svg> ... </svg>',
                showOverlay: false,
                timeout: 2000, //unblock after 2 seconds
                css: {
                    border: 0,
                    color: '#000',
                    padding: 0,
                    backgroundColor: 'transparent'
                }
            });
        });

        //Show all new requests
        const showAll = function (evt) {
            document.getElementById('newRequests').style.height = 'fit-content'
            this.innerHTML = '^';
            this.previousElementSibling.innerHTML = "Show Less";
            this.removeEventListener('click', showAll);
            this.addEventListener('click', hideAll);
        }
        //Hide all new requests
        const hideAll = function (evt) {
            document.getElementById('newRequests').style.height = '55vh'
            this.innerHTML = 'v';
            this.previousElementSibling.innerHTML = "Show All";
            this.removeEventListener('click', hideAll);
            this.addEventListener('click', showAll);
        }
        //Show all new requests action listener
        document.getElementById('allNew').addEventListener('click', showAll)



        //====================Time Countdown=======================//

        const startNightCountdown = function () {
            const time_start = new Date();
            const time_end = new Date();
            const value_start = [time_start.getHours(), time_start.getMinutes(), time_start.getSeconds()]

            const value_end = "22:00:00".split(':');

            time_start.setHours(value_start[0], value_start[1], value_start[2], 0)
            time_end.setHours(value_end[0], value_end[1], value_end[2], 0)

            //Difference in milliseconds
            const diff = time_end - time_start;

            var hours = Math.floor(diff / 3600000);
            var minutes = Math.floor(((diff / 3600000 - Math.floor(diff / 3600000)) * 60).toFixed(1));
            var seconds = Math.floor((((diff / 3600000 - Math.floor(diff / 3600000)) * 60).toFixed(1) - minutes) * 60);
            document.getElementById('seconds').innerHTML = seconds;
            document.getElementById('minutes').innerHTML = minutes;
            document.getElementById('hours').innerHTML = hours;



            const interval1 = setInterval(() => {
                document.getElementById('seconds').innerHTML = --seconds;
                if (seconds == 0) {
                    seconds = 59;
                    document.getElementById('minutes').innerHTML = --minutes;
                    if (minutes == 0) {
                        document.getElementById('hours').innerHTML = --hours;
                        minutes = 59;
                    }
                }
            }, 1000)
        }
        startNightCountdown();
    </script>
